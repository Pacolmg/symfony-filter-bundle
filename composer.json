{
    "name": "slx/filter-bundle",
    "description": "A wee library to filter objects in Symfony 4 entities repositories",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Pacolmg",
            "email": "pacolmg@gmail.com"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "symfony/config": "^4.0",
        "symfony/dependency-injection": "^4.0",
        "symfony/http-kernel": "^4.0"
    },
    "autoload": {
        "psr-4": {
            "Pacolmg\\FilterBundle\\": "src/"
        }
    },
    "autoload-dev": {
    "psr-4": {
        "Pacolmg\\FilterBundle\\Tests\\": "tests/"
    }
},
    "require-dev": {
        "symfony/phpunit-bridge": "^4.3"
    }
}
