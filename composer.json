{
    "name": "pacolmg/symfony-filter-bundle",
    "description": "Wee library to filter objects in Symfony 4 entities repositories",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Pacolmg",
            "email": "pacolmg@gmail.com"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "symfony/config": "^3.4 || ^4.0 || ^4.3  || ^4.4",
        "symfony/dependency-injection": "^3.4 || ^4.2 || ^4.3  || ^4.4",
        "symfony/http-kernel": "^3.4 || ^4.0 || ^4.3  || ^4.4"
    },
    "autoload": {
        "psr-4": {
            "Pacolmg\\SymfonyFilterBundle\\": "src/"
        }
    },
    "autoload-dev": {
    "psr-4": {
        "Pacolmg\\SymfonyFilterBundle\\Tests\\": "tests/"
    }
},
    "require-dev": {
        "symfony/phpunit-bridge": "^3.4 || ^4.2 || ^4.3  || ^4.4",
        "symfony/framework-bundle": "^3.4 || ^4.2 || ^4.3  || ^4.4",
        "symfony/browser-kit": "^3.4 || ^4.0 || ^4.3  || ^4.4"
    }
}
